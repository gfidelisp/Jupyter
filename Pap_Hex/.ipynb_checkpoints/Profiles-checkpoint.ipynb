    "\n",
    "ax1.text(Theta[250]*0.9,1.15,r'$\\tau_\\mathrm{B}$',size = 12)\n",
    "ax2.text(Theta[750]*0.98,-1.05,r'$\\tau_\\mathrm{F}$',size = 12)\n",
